{
  "name": "react-umeditor",
  "version": "0.5.15",
  "description": "React Editor like UMEditor",
  "main": "lib/editor.js",
  "private": false,
  "scripts": {
    "test": "echo \"no tests yet\" && exit 0",
    "build": "gulp clean && NODE_ENV=development gulp build && cp -R images/. dist/images && cp -R third-part/. dist/third-part",
    "build-win": "gulp clean && set NODE_ENV=development && gulp build && xcopy images dist\\images\\ /s /y && xcopy  third-part dist\\third-part\\ /s /y",
    "examples": "gulp dev:server",
    "publish:site": "gulp publish:examples",
    "start": "gulp dev",
    "release": "gulp release",
    "watch": "gulp watch:lib"
  },
  "author": [
    {
      "name": "Holly Liu",
      "email": "liuhong1.happy@163.com"
    },
    {
      "name": "Lei Haohao",
      "email": "113864965@qq.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/liuhong1happy/react-umeditor.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "react-editor",
    "react-component"
  ],
  "dependencies": {
    "classnames": "^2.2.1",
    "events": "^1.1.1",
    "hmacsha1": "^1.0.0",
    "object-assign": "^4.1.0"
  },
  "peerDependencies": {
    "react": ">=0.14.3",
    "react-dom": ">=0.14.3"
  },
  "devDependencies": {
    "envify": "^3.4.1",
    "gulp": "^3.9.0",
    "react": ">=0.14.3",
    "react-dom": ">=0.14.3",
    "react-component-gulp-tasks": "^0.7.7",
    "reactify": "^1.1.1"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      "envify"
    ]
  },
  "browserify-shim": {
    "react": "global:React"
  }
}
