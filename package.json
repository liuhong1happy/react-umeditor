{
  "name": "react-umeditor",
  "version": "1.0.0-rc1",
  "description": "React Editor like UMEditor",
  "main": "lib/editor.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "test": "mocha test/{,**/}{,**/}*.js  --compilers js:babel-register --com --require ./test/setup.js",
    "clean": "rm -rf ./lib",
	"clean-win": "del lib /Q",
    "copy": "cp -R images/. dist/images && cp -R less/. dist/less && cp -R third-part/. dist/third-part",
    "build": "npm run copy & NODE_ENV=production babel ./src --out-dir ./lib ",
    "copy-win": "xcopy less dist\\less\\ /s /y && xcopy images dist\\images\\ /s /y && xcopy  third-part dist\\third-part\\ /s /y",
    "build-win": " npm run copy-win &&  set NODE_ENV=production babel ./src --out-dir ./lib && echo '' > ./lib/editor.js &&  babel ./src/editor.js --out-file ./lib/editor.js",
    "example": "webpack-dev-server --devtool eval --progress --hot --host 0.0.0.0 --config webpack.example.config.js"
  },
  "devDependencies": {
    "babel": "^6.0.0",
    "babel-cli": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chai": "^3.5.0",
    "console-polyfill": "^0.2.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "es5-shim": "^4.4.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "open-browser-webpack-plugin": "0.0.2",
    "path": "^0.12.7",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuhong1happy/react-umeditor.git"
  },
  "keywords": [
    "react",
    "react-umeditor"
  ],
  "author": "Holly Liu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liuhong1happy/react-umeditor/issues"
  },
  "homepage": "https://github.com/liuhong1happy/react-umeditor#readme",
  "dependencies": {
    "hmacsha1": "^1.0.0"
  }
}
